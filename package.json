{
  "name": "linqua",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --display-optimization-bailout",
    "build:analyze": "npm run build -- --env.analyze",
    "build:prod": "webpack --env.prod --env.aot",
    "build:prod:analyze": "npm run build:prod -- --env.analyze",
    "tslint": "tslint",
    "lint": "npm run tslint -- --type-check --project tsconfig.json",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "protractor": "protractor",
    "e2e": "npm-run-all -p -r start protractor",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "ci": "npm run lint && npm run build && npm run test && npm run build:prod",
    "predeploy": "npm run build:prod",
    "deploy": "firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/material": "2.0.0-beta.7",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/router": "4.3.1",
    "angularfire2": "4.0.0-rc.1",
    "core-js": "2.4.1",
    "firebase": "4.1.3",
    "promise-polyfill": "^6.0.2",
    "rxjs": "5.4.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.1",
    "@angular/compiler-cli": "4.3.1",
    "@types/jasmine": "2.5.43",
    "@types/node": "8.0.14",
    "autodll-webpack-plugin": "0.2.1",
    "autoprefixer": "7.1.2",
    "circular-dependency-plugin": "3.0.0",
    "clean-webpack-plugin": "0.1.16",
    "codelyzer": "3.1.2",
    "css-loader": "0.28.4",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.2",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "json-loader": "0.5.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.3",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.5",
    "npm-run-all": "^4.0.2",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "2.0.6",
    "postcss-url": "7.1.0",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
