{
  "name": "linqua",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --port=4200",
    "build": "webpack",
    "build:analyze": "npm run build -- --env.analyze",
    "build:prod": "webpack --env.prod --env.aot",
    "build:prod:analyze": "npm run build:prod -- --env.analyze",
    "tslint": "tslint",
    "lint": "npm run tslint -- --type-check --project tsconfig.json \"src/**/*.ts\"",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "protractor": "protractor",
    "e2e": "npm-run-all -p -r start protractor",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "ci": "npm run lint && npm run build && npm run test && npm run e2e && npm run build:prod",
    "predeploy": "npm run build:prod",
    "deploy": "firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.0.3",
    "@angular/compiler": "4.0.3",
    "@angular/core": "4.0.3",
    "@angular/forms": "4.0.3",
    "@angular/http": "4.0.3",
    "@angular/platform-browser": "4.0.3",
    "@angular/platform-browser-dynamic": "4.0.3",
    "@angular/router": "4.0.3",
    "angularfire2": "^2.0.0-beta.8",
    "core-js": "2.4.1",
    "firebase": "^3.8.0",
    "rxjs": "5.3.0",
    "zone.js": "0.8.8"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.3",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.13",
    "autoprefixer": "6.7.7",
    "clean-webpack-plugin": "0.1.16",
    "codelyzer": "3.0.0",
    "css-loader": "0.28.0",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "4.0.0",
    "json-loader": "0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.3",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.3",
    "npm-run-all": "^4.0.2",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "1.3.3",
    "postcss-url": "6.0.4",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.3",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "stylus-loader": "3.0.1",
    "ts-node": "3.0.2",
    "tslib": "^1.6.1",
    "tslint": "5.1.0",
    "tslint-eslint-rules": "^4.0.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.2.2",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-bundle-analyzer": "2.4.0",
    "webpack-dev-server": "2.4.4"
  }
}
