{
  "name": "linqua",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "run-p --race start:api start:ng:local-api",
    "start:ng:local-api": "ng serve --proxy-config proxy.conf.local.json",
    "start:ng:prod-api": "ng serve --proxy-config proxy.conf.prod.json",
    "start:prod": "http-server dist/wwwroot -p 8080 --proxy https://linqua-app.com",
    "start:api": "firebase serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "http-server": "http-server",
    "npm-run-all": "npm-run-all",
    "ci": "yarn run lint && yarn run build && yarn run build:prod",
    "predeploy": "yarn run build:prod",
    "deploy": "firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cdk": "5.2.4",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "5.2.4",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/service-worker": "5.2.9",
    "core-js": "^2.4.1",
    "firebase": "^4.6.1",
    "ng-firebase-lite": "^1.0.2",
    "rxjs": "^5.5.10",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@firebase/app-types": "^0.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.6",
    "codelyzer": "~4.0.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.3.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "2.6.2"
  }
}
